              "background": "#1b1b1b"
            },
            ".mbr-gallery-item": {
              "overflow": "hidden"
            },
            ".mbr-gallery-item img": {
              "transition": "all 0.3s",
              "border-radius": "10px"
            },
            ".mbr-gallery-filter": {
              "padding-top": "20px",
              "padding-bottom": "20px"
            },
            ".mbr-gallery-item > div::before": {
              "content": "''",
